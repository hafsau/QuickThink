{
  "name": "quickthink-v1-playful",
  "version": "1.0.0",
  "description": "Quick Think - Playful variant: Speed category game with duplicate elimination",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "node test-comprehensive.js 1"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/*.test.js"
    ],
    "collectCoverageFrom": [
      "game/**/*.js",
      "!**/node_modules/**"
    ]
  },
  "dependencies": {
    "express": "^4.18.2",
    "qrcode": "^1.5.3",
    "wink-lemmatizer": "^3.0.4",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "jest": "^30.2.0"
  }
}
